<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.SignUpController">
    <HBox>
        <AnchorPane>
            <ImageView fx:id="metro1" preserveRatio="true" fitHeight="800"
                       AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0"/>
            <ImageView fx:id="logo" fitHeight="100" fitWidth="100" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="20"/>
        </AnchorPane>
        <AnchorPane>
            <VBox>
                <Label text="Sign Up" fx:id="titleLabel" AnchorPane.leftAnchor="600" AnchorPane.topAnchor="20" textFill="#00305f"/>
            </VBox>
        </AnchorPane>
    </HBox>
</AnchorPane>
